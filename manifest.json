{
	"manifest_version": 2,
	"name": "Zaycev.net Parser",
	"description": "Парсер для сайта zaycev.net",
	"version": "0.0.1",
	"content_scripts": [{
		"matches": ["*://zaycev.net/*", "*://localhost/"],
		"css": ["content.css"],
		"js": ["content.js"]
	}],
  	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": ["http://zaycev.net/", "http://localhost", "activeTab"]
}